<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.smxy.newsserver.dao.NewsTypeDao">
    <select id="findById" parameterType="Integer" resultType="NewsType">
        select * from newstype where id=#{id}
    </select>

    <select id="findAllNewsType"  resultType="NewsType">
        select * from newstype
    </select>
</mapper>